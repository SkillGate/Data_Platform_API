{
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    { "src": "/", "dest": "main.py" },
    { "src": "/github/collaboratorsCommitCount", "dest": "main.py" },
    { "src": "/github/collaboratorsCommitDetails", "dest": "main.py" },
    { "src": "/github/organizationLanguages", "dest": "main.py" },
    { "src": "/github/userInfo", "dest": "main.py" },
    { "src": "/github/projects", "dest": "main.py" },
    { "src": "/blog/posts", "dest": "main.py" },
    { "src": "/linkedIn/skills", "dest": "main.py" },
    { "src": "/linkedIn/recommendations", "dest": "main.py" },
    { "src": "/linkedIn/skillsandRecommendations", "dest": "main.py" }
  ]
}